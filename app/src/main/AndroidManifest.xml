<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="projetandroid.entmobile" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_ubp"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Connexion"
            android:label="@string/app_name"
            android:screenOrientation="userPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Accueil"
            android:label="@string/label_accueil"
            android:screenOrientation="userLandscape">
        </activity>

        <activity android:name=".PlanningProf"
            android:label="@string/label_planning_prof"
            android:screenOrientation="userLandscape">
        </activity>

        <activity android:name=".PlanningSalle"
            android:label="@string/label_planning_salle"
            android:screenOrientation="userLandscape">
        </activity>

        <activity android:name=".Reservation"
            android:label="@string/label_reservation"
            android:screenOrientation="userLandscape">
        </activity>

        <activity android:name=".Notes"
            android:label="@string/label_notes"
            android:screenOrientation="userLandscape">
        </activity>

        <activity android:name=".Profil"
            android:label="@string/label_profil"
            android:screenOrientation="userLandscape">
        </activity>

        <activity android:name=".ListeUE"
            android:label="@string/label_listeUE"
            android:screenOrientation="userLandscape">
        </activity>
    </application>

</manifest>
